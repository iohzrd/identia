{
  "name": "identia",
  "version": "0.4.32",
  "description": "Decentralized, censorship resistant social media on IPFS",
  "productName": "identia",
  "author": "iohzrd <iohzrd@gmail.com>",
  "type": "module",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "bump": "node scripts/bump-versions.js && npm run update",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "compile": "cd src/lib/flatbuffers; flatc --ts messages.fbs",
    "dev": "vite",
    "info": "tauri info",
    "postinstall": "npm run renameIpfsBinary",
    "preview": "vite preview",
    "renameIpfsBinary": "node scripts/rename-ipfs-binary.js",
    "start": "tauri dev",
    "tauri": "tauri",
    "tauri:build": "tauri build -v",
    "tauri:build-debug": "tauri build --debug -v",
    "tauri:dev": "tauri dev -v",
    "update": "ncu -u && npm install && cd src-tauri && cargo update"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0-alpha.0"
  },
  "devDependencies": {
    "@libp2p/peer-id": "^2.0.1",
    "@ltd/j-toml": "^1.38.0",
    "@splidejs/svelte-splide": "^0.2.9",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.5.6",
    "@tauri-apps/cli": "^2.0.0-alpha.1",
    "carbon-components-svelte": "^0.72.3",
    "carbon-icons-svelte": "^11.4.0",
    "carbon-preprocess-svelte": "^0.9.1",
    "execa": "^7.0.0",
    "ext2mime": "^2.0.0",
    "flatbuffers": "^23.1.21",
    "get-video-id": "^3.6.5",
    "go-ipfs": "^0.18.1",
    "ipfs-http-client": "^60.0.0",
    "is-ipfs": "^8.0.1",
    "it-all": "^2.0.0",
    "linkify-html": "^4.1.0",
    "semver": "^7.3.8",
    "string-strip-html": "^13.1.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-inview": "^3.0.2",
    "svelte-lite-youtube-embed": "^1.1.0",
    "svelte-preprocess": "^5.0.1",
    "svelte-tauri-filedrop": "^1.0.0",
    "tauri-plugin-sql-api": "github:tauri-apps/tauri-plugin-sql",
    "timeago.js": "^4.0.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "uint8arrays": "^4.0.3",
    "vite": "^4.1.1"
  }
}